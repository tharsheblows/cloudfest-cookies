{
  "manifest_version": 3,
  "name": "Cloudfest Cookies",
  "description": "View cookies your website is running.",
  "version": "0.1",
  "action": {
    "default_popup": "index.html",
    "default_icon": "take-a-bite.png",
    "default_title": "Cookies cookies cookies"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "contextMenus",
    "cookies",
    "history",
    "webRequest",
    "declarativeNetRequest"
  ],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "sample.js",
    "type": "module"
  }
}
